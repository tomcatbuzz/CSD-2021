header-height: 100px;\r\n  --burger-width: 40px;\r\n  --font-headings: \"Poppins\", serif;\r\n\r\n  --bcg-green: #acb7ae;\r\n  --bcg-lightgreen: #a6b3b3;\r\n  --bcg-lightbrown: #b0a7a4;\r\n  --bcg-lightblue: #a3abb1;\r\n\r\n  --bcg-loaderblue: #758a94;\r\n  --bcg-loaderbluedark: #566c76;\r\n  --bcg-darker: #212121;\r\n  --bcg-fill-color: var(--bcg-green);\r\n\r\n  --loader-width: 27.36vw;\r\n  --loader-height: 50vh;\r\n\r\n  --v-spacing: 10vh;\r\n  --h-gutter: 6.6vw;\r\n\r\n  --easing: cubic-bezier(0.5, 0.7, 0.4, 1);\r\n}\r\n\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n  font-family: Georgia, \"Times New Roman\", Times, serif;\r\n}\r\n\r\n/* hide page content by default */\r\n.is-loading #main,\r\n.is-loading .logo,\r\n.is-loading .burger,\r\n.is-loading .nav,\r\n.is-loading .canvas,\r\n.is-loading #container,\r\n.is-loading .fill-background {\r\n  display: none;\r\n}\r\n.is-loading {\r\n  background-color: var(--bcg-loaderblue);\r\n}\r\n.is-transitioning {\r\n  pointer-events: none;\r\n  cursor: progress;\r\n}\r\n\r\n#container {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: -1;\r\n  height: 100vh;\r\n  width: 100vw;\r\n}\r\ncanvas {\r\n  display: block;\r\n}\r\n/* .canvas {\r\n  opacity: 0.5;\r\n} */\r\n\r\n/* .canvas {\r\n  position: fixed;\r\n  z-index: -1;\r\n  width: 100vw;\r\n  height: 100vh;\r\n  background: green;\r\n}\r\n.canvas:before,\r\n.canvas:after {\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 30vh;\r\n  background: linear-gradient(\r\n    180deg,\r\n    rgba(0, 0, 0, 0) 0%,\r\n    rgba(0, 0, 0, 0.8421743697478992) 100%\r\n  );\r\n  content: \" \";\r\n}\r\n.canvas:after {\r\n  bottom: auto;\r\n  top: 0;\r\n  background: linear-gradient(\r\n    180deg,\r\n    rgba(0, 0, 0, 0.8) 0%,\r\n    rgba(0, 0, 0, 0) 100%\r\n  );\r\n} */\r\n\r\n.nav {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  display: flex;\r\n  justify-content: center;\r\n  list-style: none;\r\n  width: 100%;\r\n  margin: 0;\r\n  padding: 2em 0;\r\n}\r\n\r\n.nav a {\r\n  text-decoration: underline;\r\n  color: #fff;\r\n  padding: 0 1em;\r\n  display: block;\r\n  transition: all 0.3s ease-in-out;\r\n  font-size: 4vw;\r\n}\r\n\r\n.nav [aria-current]{\r\n  text-decoration: none;\r\n  transform: scale(1.5);\r\n}\r\n.nav a:hover{\r\n  text-decoration: none;\r\n}\r\n\r\n/* .nav li {\r\n  list-style-type: none;\r\n} */\r\n\r\nh1 {\r\n  text-align: center;\r\n  overflow: hidden;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  color:#fff;\r\n  font-size: 6vh;\r\n  z-index: 1;\r\n}\r\n\r\n.slider {\r\n  position: inherit;\r\n  display: flex;\r\n  flex: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  bottom: 0;\r\n  left: 0;\r\n  width: 100vw;\r\n  height: 100vh;\r\n}\r\n\r\n/* =Loader */\r\n.loader,\r\n.loader__content {\r\n  min-height: 100vh;\r\n  position: fixed;\r\n  flex-direction: column;\r\n  z-index: 4;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  visibility: hidden;\r\n  overflow: hidden;\r\n}\r\n\r\n.loader__mask {\r\n  position: absolute;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: var(--bcg-loaderblue) url(\"../img/Group 1.svg\") no-repeat center;\r\n  background-size: 155px 155px;\r\n  overflow: hidden;\r\n}\r\n.loader .inner,\r\n.loader__content .inner {\r\n  width: var(--loader-width);\r\n  height: var(--loader-height);\r\n}\r\n.loader .inner {\r\n  background-color: var(--bcg-loaderbluedark);\r\n  position: relative;\r\n}\r\n.loader .progress {\r\n  background-color: var(--bcg-lightblue);\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  width: var(--loader-width);\r\n  height: var(--loader-height);\r\n}\r\n.loader__content {\r\n  z-index: 4;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background: transparent;\r\n  visibility: hidden; /* hide loader content by default */\r\n}\r\n.loader__title {\r\n  font-size: 7vw;\r\n  line-height: 8vw;\r\n  margin: 0;\r\n  font-family: var(--font-headings);\r\n  color: var(--text-color);\r\n  font-weight: normal;\r\n  position: absolute;\r\n  text-align: center;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n  z-index: 5;\r\n}\r\n.loader__image {\r\n  width: var(--loader-width);\r\n  padding-bottom: 50vh;\r\n  overflow: hidden;\r\n  position: relative;\r\n}\r\n.loader__title--mask {\r\n  overflow: hidden;\r\n}\r\n.loader__title--mask span {\r\n  display: block;\r\n}\r\n.loader__image--mask {\r\n  overflow: hidden;\r\n  width: 100%;\r\n  height: 100%;\r\n  position: absolute;\r\n}\r\n\r\n@media screen and (max-width: 1042px) {\r\n  .loader .inner,\r\n  .loader__content .inner,\r\n  .loader .progress,\r\n  .loader__image {\r\n    width: 47.36vw;\r\n  }\r\n}\r\n\r\n#container {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: -1;\r\n  height: 100vh;\r\n  width: 100vw;\r\n}\r\ncanvas {\r\n  display: block;\r\n}\r\n/* .canvas {\r\n  opacity: 0.5;\r\n} */\r\n\r\n/* .canvas {\r\n  position: fixed;\r\n  z-index: -1;\r\n  width: 100vw;\r\n  height: 100vh;\r\n  background: green;\r\n}\r\n.canvas:before,\r\n.canvas:after {\r\n  position: absolute;\r\n  bottom: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 30vh;\r\n  background: linear-gradient(\r\n    180deg,\r\n    rgba(0, 0, 0, 0) 0%,\r\n    rgba(0, 0, 0, 0.8421743697478992) 100%\r\n  );\r\n  content: \" \";\r\n}\r\n.canvas:after {\r\n  bottom: auto;\r\n  top: 0;\r\n  background: linear-gradient(\r\n    180deg,\r\n    rgba(0, 0, 0, 0.8) 0%,\r\n    rgba(0, 0, 0, 0) 100%\r\n  );\r\n} */\r\n"},"lineCount":null}},"error":null,"hash":"a36091cefe25439dcf7f64c2cc02857c","cacheData":{"env":{}}}